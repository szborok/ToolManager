{
  "name": "tool-manager",
  "version": "2.0.0",
  "description": "CNC ToolManager with organized temp structure for BRK CNC Management Dashboard",
  "main": "main.js",
  "scripts": {
    "setup-test-data": "node scripts/setup-test-data.js",
    "start": "node main.js --auto",
    "auto": "node main.js --auto",
    "manual": "node main.js --manual",
    "setup": "node main.js --setup",
    "cleanup": "node main.js --cleanup",
    "test": "node scripts/setup-test-data.js && node main.js --test",
    "test:no-setup": "node main.js --test",
    "debug": "node main.js --debug",
    "test-storage": "node main.js --test-storage",
    "list-results": "node main.js --list-results",
    "export-results": "node main.js --export-results"
  },
  "keywords": [
    "cnc",
    "excel-processing",
    "manufacturing",
    "tool-management",
    "temp-structure",
    "cross-platform",
    "brk-dashboard"
  ],
  "author": "BRK CNC Management Dashboard",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.6.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "chalk": "^4.1.2",
    "mongodb": "^6.3.0",
    "exceljs": "^4.4.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
